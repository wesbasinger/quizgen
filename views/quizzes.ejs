<!DOCTYPE html>
<html lang="en">
<head>
    <% include partials/head %>
</head>
<body class="container">

    <header>
        <% include partials/header %>
    </header>

    <main>
        <div class="row">
            <div class="col-sm-8">

                <div class="jumbotron">
                    <h1>All Quizzes</h1>
                </div>

            </div>
        </div>
        <div class="row">
          <div class="col-sm-8">
            <ul>
              <% data.forEach(function(quiz) { %>
                <li><a href="/quiz/<%= quiz.slug %>/jwt/<%=jwt%>"><%= quiz.slug %></a></li>
              <% }); %>
            </ul>
          </div>
        </div>
    </main>

    <footer>
        <% include partials/footer %>
    </footer>

</body>
</html>
